    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "description": "SkyRoads clone in javascript",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/workco/skyroads.git"
