{
  "name": "skyroads",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "main": "build/server.js",
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "webpack-dev-server",
    "watch": "cross-env HYDRATE=\"true\" webpack",
    "build": "npm run clean && cross-env NODE_ENV=\"production\" webpack",
    "build-hydrate": "npm run clean && cross-env HYDRATE=\"true\" cross-env NODE_ENV=\"production\" webpack",
    "preview": "npm run clean && cross-env NODE_ENV=\"production\" webpack-dev-server",
    "clean": "rm -rf ./build",
    "lint-hook": "eslint ./source/js ./webpack ./*.js -f table --ext .js --ext .jsx",
    "lint": "npm run lint-hook --silent || true",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
